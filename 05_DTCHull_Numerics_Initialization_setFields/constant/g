FoamFile
{
    version     2.0;
    format      ascii;
    class       uniformDimensionedVectorField;
    object      g;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
// Acceleration due to gravity
dimensions      [0 1 -2 0 0 0 0]; // m/s^2
value           (0 0 -9.81); // negative z-dir


