FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
deltaT          1;

writeInterval   100;

// added for snappyHexMesh stage
writeFormat     binary;
writePrecision  6;  // write tolerance needs to be higher than mergeTolerance in "snappyHexMeshDict".
writeCompression off;